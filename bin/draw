#!/usr/bin/env python3
import dateutil.parser
import fileinput

import matplotlib.pyplot as plt
import pandas as pd


def yield_lines():
    for line in fileinput.input():
        date, value = line.split(',')
        yield pd.to_datetime(date), value


if __name__ == '__main__':
    raw_points = list(yield_lines())
    series = pd.Series([p[1] for p in raw_points], index=[p[0] for p in raw_points])
    plt.plot(series)
    plt.show()
